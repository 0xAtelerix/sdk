# Chain Configuration (works with any EVM-compatible chain)
RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY
PRIVATE_KEY=your_private_key_without_0x_prefix

# Block Explorer API Key (for contract verification)
# Key from any scan like etherscan, polygonscan will work
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY

# Source Chain ID for AppChain registration (defaults to 42 if not set)
SOURCE_CHAIN_ID=42
